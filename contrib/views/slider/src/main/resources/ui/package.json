    "javascript-brunch": ">= 1.0 < 1.8",
    "stylus-brunch": ">= 1.0 < 1.8",
    "uglify-js-brunch": ">= 1.0 < 1.8"
  },
  "devDependencies": {
    "karma": "*",
    "karma-phantomjs-launcher": "~0.1.2",
    "karma-qunit": "*",
    "phantomjs": "^1.9.2"
